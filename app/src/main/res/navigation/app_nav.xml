  <fragment
    android:id="@+id/nav_main"
    android:name="com.github.satoshun.example.tablayout.TabLayoutFragment" />

  <fragment
    android:id="@+id/nav_fake_drag"
    android:name="com.github.satoshun.example.fake.FakeDragFragment" />

  <fragment
    android:id="@+id/nav_main2"
    android:name="com.github.satoshun.example.MainFragment2" />

  <fragment
    android:id="@+id/nav_infinite"
    android:name="com.github.satoshun.example.infinite.InfiniteFragment" />

  <fragment
    android:id="@+id/nav_indicator"
    android:name="com.github.satoshun.example.indicator.IndicatorFragment" />

  <fragment
    android:id="@+id/nav_view_pager_in_view_pager2"
    android:name="com.github.satoshun.example.viewpager.ViewPagerInViewPager2Fragment" />

  <activity
    android:id="@+id/nav_child"
    android:name="com.github.satoshun.example.child.ChildActivity" />

  <activity
    android:id="@+id/nav_limit"
    android:name="com.github.satoshun.example.limit.LimitActivity" />

  <fragment
    android:id="@+id/nav_material_motion"
